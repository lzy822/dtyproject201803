<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PopWindowForListShow">
    <com.bin.david.form.core.SmartTable
        android:id="@+id/table"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />

    <com.github.clans.fab.FloatingActionButton
        android:id="@+id/outputData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:src="@drawable/ic_call_merge_white_24dp"
        android:visibility="visible"
        app:fab_colorNormal="#F50057"
        app:fab_colorPressed="#E8004A"
        app:fab_colorRipple="#C20024"
        app:fab_elevationCompat="6dp"
        app:fab_shadowColor="#000"
        app:fab_size="mini" />

    <com.github.clans.fab.FloatingActionButton
        android:id="@+id/saveData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/outputData"
        android:src="@drawable/ic_save_white_24dp"
        android:visibility="visible"
        app:fab_colorNormal="#F50057"
        app:fab_colorPressed="#E8004A"
        app:fab_colorRipple="#C20024"
        app:fab_elevationCompat="6dp"
        app:fab_shadowColor="#000"
        app:fab_size="mini" />
</RelativeLayout>